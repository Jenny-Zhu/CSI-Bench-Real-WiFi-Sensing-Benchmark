{
  "pipeline": "supervised",
  "s3_data_base": "s3://rnd-sagemaker/Data/Benchmark/",
  "s3_output_base": "s3://rnd-sagemaker/Benchmark_Log/",
  "model": "transformer",
  "win_len": 500,
  "feature_size": 232,
  "seed": 42,
  "batch_size": 128,
  "epochs": 100,
  "learning_rate": 1e-3,
  "weight_decay": 1e-5,
  "warmup_epochs": 5,
  "patience": 15,
  "test_splits": "all",
  "instance_type": ["ml.g5.16xlarge","ml.g5.8xlarge","ml.g4dn.8xlarge"],
  "instance_count": 1,
  "framework_version": "1.12.1",
  "py_version": "py38",
  "base_job_name": "wifi-sensing",
  "available_models": ["mlp", "lstm", "resnet18", "transformer", "vit", "patchtst", "timesformer1d"],
  "available_tasks": [
    "ProximityRecognition","HumanActivityRecognition","HumanIdentification"
  ],
  "fewshot": {
    "enabled": false,
    "k_shots": 5,
    "adaptation_lr": 0.01,
    "adaptation_steps": 10,
    "support_split": "val_id",
    "query_split": "test_cross_env",
    "finetune_all": false,
    "eval_shots": false
  },
  "file_format": "h5",
  "num_workers": 4
} 

